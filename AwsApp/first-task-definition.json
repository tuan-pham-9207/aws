{
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "family": "task-definition-4",
  "containerDefinitions": [
    {
      "name": "container-4",
      "image": "560345640735.dkr.ecr.ap-south-1.amazonaws.com/repository-1",
      "essential": true,
      "cpu": 256,
      "memory": 512,
      "memoryReservation": 256,
      "portMappings": [
        {
          "name": "nginx-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/task1",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      }
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "0.5 GB",
  "cpu": "0.25 vCPU",
  "taskRoleArn": "arn:aws:iam::560345640735:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::560345640735:role/ecsTaskExecutionRole"
}